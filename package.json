{
  "name": "lighthouse-vsts",
  "scripts": {
    "postinstall": "cd task && npm install",
    "package": "rimraf *.vsix && tsc -p task && tsc -p web && tfx extension create --output-path PackageOutput",
    "bump-version": "node version-bump.js",
    "publish": "tfx extension publish --publisher gsoft",
    "publish:dev": "tfx extension publish --publisher gsoft-dev",
    "test": "mocha task/test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "vss-web-extension-sdk": "5.141.0"
  },
  "devDependencies": {
    "@types/mocha": "10.0.1",
    "@types/node": "18.13.0",
    "husky": "8.0.3",
    "minimist": "1.2.8",
    "mocha": "10.2.0",
    "prettier": "2.8.4",
    "pretty-quick": "3.1.3",
    "rimraf": "4.1.2",
    "tfx-cli": "0.12.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
